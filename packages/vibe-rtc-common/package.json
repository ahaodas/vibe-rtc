{
    "name": "vibe-rtc-common",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./core": {
            "types": "./dist/core.d.ts",
            "import": "./dist/core.mjs",
            "require": "./dist/core.cjs"
        },
        "./firebase": {
            "types": "./dist/firebase.d.ts",
            "import": "./dist/firebase.mjs",
            "require": "./dist/firebase.cjs"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsup src/index.ts src/core.ts src/firebase.ts --dts --format esm,cjs --clean",
        "test": "echo \"(no tests here)\""
    },
    "dependencies": {
        "@vibe-rtc/rtc-core": "workspace:^",
        "@vibe-rtc/rtc-firebase": "workspace:^"
    }
}
